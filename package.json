{
  "name": "react-uptime-status",
  "version": "0.0.2",
  "description": "An opinionated uptime status display for React.",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx",
    "dev": "tsup src/index.tsx --watch",
    "format": "prettier --write ."
  },
  "keywords": [
    "react",
    "status",
    "uptime",
    "monitoring"
  ],
  "author": "Rory Flint <https://roryflint.co.uk>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruairidhflint/react-uptime-status.git"
  },
  "bugs": {
    "url": "https://github.com/ruairidhflint/react-uptime-status/issues"
  },
  "devDependencies": {
    "@types/node": "^18.11.13",
    "@types/react": "^18.3.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "packageManager": "pnpm@9.9.0"
}
